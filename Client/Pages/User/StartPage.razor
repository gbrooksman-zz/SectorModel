@page "/User/StartPage"

@inject AppSettings appSettings
@inject UserState userState

@using SectorModel.Shared.Entities
@using SectorModel.Client.Pages.Models

<AppHeader IsStart=true/>

    <ModelList Models="@userState.ModelList" IsCore=false/>
    <hr/>
    <ModelList Models="@coreModelList" IsCore=true/>

@code
{
    List<Model> coreModelList = new List<Model>();

    protected override void OnInitialized()
    {
         coreModelList.Add(appSettings.CoreModel);
    }
}